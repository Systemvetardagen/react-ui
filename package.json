{
  "name": "@systemvetardagen/react-ui",
  "private": false,
  "version": "0.1.3",
  "description": "A reusable React UI component library for Systemvetardagen.",
  "author": "Systemvetardagen Web Development and Design",
  "license": "MIT",
  "homepage": "https://github.com/Systemvetardagen/react-ui#readme",
  "bugs": {
    "url": "https://github.com/Systemvetardagen/react-ui/issues"
  },
  "keywords": [
    "react",
    "ui",
    "component",
    "library",
    "systemvetardagen"
  ],
  "type": "module",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/ui.es.js",
      "require": "./dist/ui.umd.js"
    },
    "./styles.css": {
      "require": "./dist/styles.css",
      "default": "./dist/styles.css"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {},
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build && npm run build:css",
    "build:css": "tailwindcss -m -i ./src/tailwind-entry.css -o ./dist/styles.css",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tailwindcss/cli": "^4.1.13",
    "@tailwindcss/vite": "^4.1.13",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^9.1.8",
    "globals": "^16.4.0",
    "playwright": "^1.55.1",
    "tailwindcss": "^4.1.13",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.7",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "react-router-dom": "^7.9.2"
  }
}
